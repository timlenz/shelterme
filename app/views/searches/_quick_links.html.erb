<% if @search.se_str(50).size > 0 or @search.se_str(200).size > 0 or @search.se_str(500).size > 0 %>

<script>
$(function(){  
  // Search string (se_str)
  $('#se_str').click(function(){
    $('#waiting').show();
    $('input[id^="search"]').not('#search_search_string').not('#search_location').val("");
    $('#new_search button').removeClass('active');
    if ( $('.partialMatch span').length ) {
      nearest = $('.partialMatch span').html();
      $('#search_location').val(nearest);
    };
    $('button[type="submit"]').click();
	  $('.petTile').hide();
  });
});
</script>

