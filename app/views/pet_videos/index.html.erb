<%= provide(:title, 'All Videos') %>
<div class="container content">
  <div class="profile-section">
    <div class="pageHeader">
      <%= render "shared/header_logo" %>
      <div class="headerInfo" style="width:385px;">
        <h1><%= pluralize(PetVideo.count, "Video") %></h1>
      </div>
    </div>
    <div id="pet_videos" class="row">
      <%= render partial: 'pet_videos', collection: @pet_videos.reverse, as: :f %>
      <div class="clearfix"></div>
      <%= will_paginate %>
    </div>
  </div>
</div>